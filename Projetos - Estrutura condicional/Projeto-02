package Estrutura_condicional;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {

		Scanner sc =new Scanner(System.in);
		int numero;
		
		numero = sc.nextInt();
		
		if (numero < 0) {
		    System.out.println("negativo!");
		}	
		
		else {
		    	System.out.println("NÃ£o negativo!");
		}
		
		sc.close();
	}
}