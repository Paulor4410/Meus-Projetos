package Estrutura_condicional;

import java.util.Scanner;

public class main {

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		int N = sc.nextInt();

		if (N % 2 == 0) {
			System.out.println("PAR");
			
		} 
		else {
			System.out.println("IMPAR");
		}

		sc.close();
	}
}
